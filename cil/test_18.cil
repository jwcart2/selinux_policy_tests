(class CLASS (PERM))
(class C1 (P1A P1B P1C))
(class C2 (P2A P2B P2C))
(classorder (CLASS C1 C2))
(sid SID)
(sidorder (SID))
(user USER)
(role ROLE)
(type TYPE)
(category CAT)
(categoryorder (CAT))
(sensitivity SENS)
(sensitivityorder (SENS))
(sensitivitycategory SENS (CAT))
(allow TYPE self (CLASS (PERM)))
(roletype ROLE TYPE)
(userrole USER ROLE)
(userlevel USER (SENS))
(userrange USER ((SENS)(SENS (CAT))))
(sidcontext SID (USER ROLE TYPE ((SENS)(SENS))))

(optional op1a
  (type t1a)
  (allow t1a self (C1 (P1A)))
  (optional op1b
    (type t1b)
	(allow t1b self (C1 (P1B)))
    (optional op1c
      (type t1c)
	  (allow t1c self (C1 (P1C)))
      (allow NO_TYPE self (C1 (P1C)))
    )
	(allow NO_TYPE self (C1 (P1B)))
  )
  (allow NO_TYPE self (C1 (P1A)))
)

(optional op2a
  (type t2a)
  (allow NO_TYPE self (C2 (P2A)))
  (allow t2a self (C2 (P2A)))
  (optional op2b
    (type t2b)
	(allow NO_TYPE self (C2 (P2B)))
	(allow t2b self (C2 (P2B)))
    (optional op2c
      (type t2c)
      (allow NO_TYPE self (C2 (P2C)))
	  (allow t2c self (C2 (P2C)))
    )
  )
)
